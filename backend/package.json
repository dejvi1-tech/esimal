{
  "name": "esim-marketplace-backend",
  "version": "1.0.0",
  "description": "Backend for eSIM Marketplace application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "cnpm run typeorm migration:generate",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:sql": "ts-node src/scripts/runMigration.ts",
    "test:email": "ts-node src/scripts/testEmail.ts",
    "test:email:simple": "ts-node src/scripts/simpleEmailTest.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.49.10",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.30",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.4",
    "@types/stripe": "^8.0.416",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "stripe": "^18.2.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.24",
    "typescript": "^5.8.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0"
  }
}
